{
  "manifest_version": 3,
  "name": "browser-pipes Sender",
  "version": "1.0",
  "description": "Sends URLs to the browser-pipes Plumber.",
  "permissions": [
    "nativeMessaging",
    "contextMenus",
    "activeTab",
    "scripting",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Send to Browser Pipe"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}